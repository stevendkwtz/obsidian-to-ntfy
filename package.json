{
    "name": "obsidian-to-ntfy",
    "version": "1.0.0",
    "description": "This is a ntfy plugin for Obsidian.",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "lint": "eslint ./src --fix && tsc --noEmit --pretty",
        "build:dev": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs",
        "build:watch": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs watch",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "deploy:local:dev": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs && ./scripts/copy-build-files-to-local-vault.sh",
        "deploy:local": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && ./scripts/copy-build-files-to-local-vault.sh"
    },
    "keywords": [],
    "author": "",
    "license": "GNU GPLv3",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.17.3",
        "obsidian": "latest",
        "tslib": "2.4.0",
        "typescript": "4.7.4",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "@types/prettier": "^2.7.3",
        "prettier": "^2.8.8"
    },
    "dependencies": {
        "ntfy": "^1.5.2"
    }
}
